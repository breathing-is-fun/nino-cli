{
  "name": "nino-cli",
  "version": "0.1.1",
  "description": "A cli for Nino, just some simple conifg of webpack",
  "main": "index.js",
  "bin": {
    "nino": "./bin/nino.js"
  },
  "scripts": {
    "lint": "npx eslint .",
    "prettier": "node ./scripts/prettier.js",
    "lint-staged": "lint-staged",
    "pub": "npm config set registry=https://registry.npmjs.org && npm publish && npm config set registry=https://registry.npm.taobao.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zy410419243/nino-cli.git"
  },
  "keywords": [
    "cli"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zy410419243/nino-cli/issues"
  },
  "homepage": "https://github.com/zy410419243/nino-cli#readme",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^1.0.0",
    "commander": "^2.19.0",
    "commitizen": "^3.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "style-loader": "^0.23.0",
    "toho-log-plugin": "^1.0.11",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{js,css}": [
      "node ./scripts/prettier.js",
      "npx eslint .",
      "git add"
    ]
  }
}
